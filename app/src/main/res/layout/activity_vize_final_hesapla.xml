<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:context=".VizeFinalHesaplaActivity">

    <TextView
        android:id="@+id/VizeTextasdad"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="255dp"
        android:gravity="center_vertical"
        android:text="Vize:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/sonucText"
        app:layout_constraintEnd_toStartOf="@+id/VizeText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="327dp"
        android:gravity="center_vertical"
        android:text="Final:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/FinalText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/VizeTextasdad" />

    <EditText
        android:id="@+id/VizeText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="165dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/FinalText"
        app:layout_constraintEnd_toStartOf="@+id/VizeYuzdelikText"
        app:layout_constraintStart_toEndOf="@+id/VizeTextasdad"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/FinalText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="65dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toStartOf="@+id/FinalYuzdelikText"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/VizeText" />

    <EditText
        android:id="@+id/VizeYuzdelikText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="87dp"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/FinalYuzdelikText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/VizeText"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <EditText
        android:id="@+id/FinalYuzdelikText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="87dp"
        android:layout_marginBottom="65dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/FinalText"
        app:layout_constraintTop_toBottomOf="@+id/VizeYuzdelikText" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="61dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="101dp"
        android:layout_marginBottom="13dp"
        android:gravity="center"
        android:text="YÃ¼zdelik"
        android:textColor="#050505"
        app:layout_constraintBottom_toTopOf="@+id/VizeYuzdelikText"
        app:layout_constraintStart_toStartOf="@+id/VizeYuzdelikText"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="193dp"
        android:layout_height="73dp"
        android:layout_marginBottom="188dp"
        android:onClick="VizeFinalHesapla"
        android:text="Hesapla"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/FinalText" />

    <TextView
        android:id="@+id/sonucText"
        android:layout_width="242dp"
        android:layout_height="0dp"
        android:layout_marginBottom="105dp"
        android:textColor="#169C1C"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/VizeTextasdad" />
</androidx.constraintlayout.widget.ConstraintLayout>