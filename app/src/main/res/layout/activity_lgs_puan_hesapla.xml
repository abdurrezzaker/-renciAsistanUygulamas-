        android:text="Yabancı Dil:"
        android:textColor="#0E0E0E"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toStartOf="@+id/YabanciDogruText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="130dp"
        android:layout_height="33dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="45dp"
        android:gravity="center_vertical"
        android:text="T.C. İnk. Tar. ve Atatürk:"
        android:textColor="#0E0E0E"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/DiplomaNotuText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="130dp"
        android:layout_height="33dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="49dp"
        android:gravity="center_vertical"
        android:text="Fen Bilimleri:"
        android:textColor="#0E0E0E"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/FenDogruText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="130dp"
        android:layout_height="33dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="38dp"
        android:gravity="center_vertical"
        android:text="Matematik:"
        android:textColor="#0E0E0E"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/MatematikDogruText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/TurkceDogruText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/MatematikDogruText"
        app:layout_constraintEnd_toStartOf="@+id/TurkceYanlisText"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/MatematikDogruText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="30dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/FenDogruText"
        app:layout_constraintEnd_toStartOf="@+id/MatematikYanlisText"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/TurkceDogruText" />

    <EditText
        android:id="@+id/FenDogruText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="21dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/SosyalDogruText"
        app:layout_constraintEnd_toStartOf="@+id/FenYanlisText"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/MatematikDogruText" />

    <EditText
        android:id="@+id/SosyalDogruText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="18dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/DiplomaNotuText"
        app:layout_constraintEnd_toStartOf="@+id/SosyalYanlisText"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/FenDogruText" />

    <EditText
        android:id="@+id/YabanciDogruText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="27dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toStartOf="@+id/YabanciYanlisText"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/DiplomaNotuText" />

    <EditText
        android:id="@+id/DiplomaNotuText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="19dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/YabanciDogruText"
        app:layout_constraintEnd_toStartOf="@+id/InklapYanlisText"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/SosyalDogruText" />

    <EditText
        android:id="@+id/TurkceYanlisText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/MatematikYanlisText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/TurkceDogruText"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/MatematikYanlisText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="30dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/FenYanlisText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/MatematikDogruText"
        app:layout_constraintTop_toBottomOf="@+id/TurkceYanlisText" />

    <EditText
        android:id="@+id/FenYanlisText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="21dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/SosyalYanlisText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/FenDogruText"
        app:layout_constraintTop_toBottomOf="@+id/MatematikYanlisText" />

    <EditText
        android:id="@+id/SosyalYanlisText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="18dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/InklapYanlisText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/SosyalDogruText"
        app:layout_constraintTop_toBottomOf="@+id/FenYanlisText" />

    <EditText
        android:id="@+id/InklapYanlisText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="19dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/YabanciYanlisText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/DiplomaNotuText"
        app:layout_constraintTop_toBottomOf="@+id/SosyalYanlisText" />

    <EditText
        android:id="@+id/YabanciYanlisText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="120dp"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/YabanciDogruText"
        app:layout_constraintTop_toBottomOf="@+id/InklapYanlisText" />

    <Button
        android:id="@+id/button7"
        android:layout_width="135dp"
        android:layout_height="55dp"
        android:layout_marginBottom="38dp"
        android:onClick="lgsHesapla"
        android:text="Hesapla"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/YabanciDogruText" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginStart="184dp"
        android:gravity="center"
        android:text="Doğru"
        app:layout_constraintBottom_toTopOf="@+id/TurkceDogruText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.888" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="60dp"
        android:layout_height="35dp"
        android:layout_marginEnd="40dp"
        android:gravity="center"
        android:text="Yanlış"
        app:layout_constraintBottom_toTopOf="@+id/TurkceYanlisText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.888" />
</androidx.constraintlayout.widget.ConstraintLayout>